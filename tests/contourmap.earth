<Map name="ContourMap Example">

    <xi:include href="readymap_elevation.xml"/>

    <!-- The stops are optional. Leave them out for a default. -->
    <ContourMap name="Contour Map">
        <stops>
            <stop elevation="-7500" color="#000016ff"/>
            <stop elevation="-2500" color="#00007fff"/>
            <stop elevation="-625" color="#0000ffff"/>
            <stop elevation="0" color="#007fffff"/>
            <stop elevation="2.5" color="#c2b280ff"/>
            <stop elevation="50" color="#d6d63fff"/>
            <stop elevation="250" color="#1f9e00ff"/>
            <stop elevation="1000" color="#30632cff"/>
            <stop elevation="1750" color="#a37714ff"/>
            <stop elevation="2250" color="#6c2f2fff"/>
            <stop elevation="2600" color="#7f7f7fff"/>
            <stop elevation="3000" color="#ffffffff"/>
        </stops>
    </ContourMap>
    
    <GDALDEM name="Hill Shade" blend="modulate" opacity="0.15">
        <processing>hillshade</processing>
    </GDALDEM>
</Map>
