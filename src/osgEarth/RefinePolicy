#ifndef OSGEARTH_REFINE_POLICY_H
#define OSGEARTH_REFINE_POLICY_H 1

namespace osgEarth
{
    enum RefinePolicy
    {
        REFINE_REPLACE,  // async node replaces children
        REFINE_ADD       // async node combines with children
    };
}

#endif
